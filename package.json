{
  "name": "blog.rintaro.dev",
  "version": "0.0.1",
  "author": "@HidakaRintaro",
  "private": true,
  "packageManager": "bun@1.0.3",
  "engines": {
    "node": "20.x",
    "bun": "1.x"
  },
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "panda codegen",
    "lint": "eslint --ext .ts,.astro ./src",
    "fmt": "prettier --write 'src/**/*.{js,ts,astro}'",
    "fmt:check": "prettier --check 'src/**/*.{js,ts,astro}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,astro}'",
    "fix": "bun fmt && bun lint:fix"
  },
  "dependencies": {
    "astro": "^3.2.1"
  },
  "devDependencies": {
    "@pandacss/astro": "^0.15.4",
    "@pandacss/dev": "^0.15.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "bun-types": "^1.0.3",
    "eslint": "^8.50.0",
    "eslint-plugin-astro": "^0.29.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0"
  }
}
